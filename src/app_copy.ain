// عشان نحسب السنة اللي اتولدت فيها
دالة حساب_سنة_الميلاد(العمر: صحيح): صحيح{
	متغير سنة_الميلاد:صحيح=حساب_الطرح(2023، العمر)
	بقيمة سنة_الميلاد
}

// لو حبيت تحسب جمع رقمين س، و ص

دالة حساب_الجمع(س: صحيح، ص: صحيح){
	بقيمة س+ص
}

دالة حساب_الطرح(س: صحيح، ص: صحيح){
	بقيمة س-ص
}

دالة البداية(){
	متغير أ=50
	متغير ب:صحيح


    // حط قيمة ب
    ب=أ*5

	متغير طرح1=حساب_الطرح(أ،ب)
	متغير طرح2=حساب_الطرح(طرح1،طرح1)

    // عُمري مش 20 أوي
	ثابت سنة_الميلاد=حساب_سنة_الميلاد(20)
}