تصنيف طالب{
	متغير الاسم=""
	متغير العمر=0

	دالة طباعة(){
		اظهر_( "أنا الطالب \""+الاسم+"\" وعمري "+العمر.كنص())
	}
	
}


دالة البداية(){
	متغير _طالب=إنشاء طالب()
	اظهر("أدخل اسم الطالب الأول: ")
    _طالب.الاسم=أدخل()
    اظهر("أدخل عمره: ")
	_طالب.العمر=أدخل().كصحيح()
	اظهر("أدخل اسم الطالب الثاني: ")
	متغير _طالب2=إنشاء طالب()
	_طالب2.الاسم=أدخل()
	اظهر("أدخل عمره: ")
	_طالب2.العمر=أدخل().كصحيح()
	_طالب2.طباعة()
	_طالب.طباعة()
}
