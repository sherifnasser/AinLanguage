تصنيف طالب{
	إنشاء(){
		اظهر_("إنشاء()")
	}
	
	إنشاء(_الاسم:نص، _العمر:صحيح){
		الاسم=_الاسم
		العمر=_العمر
		اظهر_("إنشاء(_الاسم:نص، _العمر:صحيح)")
		أرجع اظهر_("ت")
	}
	
	متغير الاسم=""
	متغير العمر=0

}

دالة البداية(){
	متغير _طالب=إنشاء طالب("شريف"،20)
	اظهر_(_طالب.الاسم)
	اظهر_(_طالب.العمر)
	
	متغير _طالب2=إنشاء طالب()
	اظهر_(_طالب2.الاسم)
	اظهر_(_طالب2.العمر)
}