تصنيف طالب{

	متغير الاسم="شريف"
	متغير العمر=0
	
	إنشاء(_العمر:صحيح){
		العمر=_العمر
	}
}

دالة تغيير(_طالب:طالب){
	_طالب.العمر=30
}

دالة تجربة():طالب{
	اظهر_("تجربة")
	أرجع إنشاء طالب(18)
}

دالة البداية(){

	متغير _طالب=إنشاء طالب(20)
	
	اظهر_(_طالب.الاسم)
	اظهر_(_طالب.العمر)
	تغيير(_طالب)
	_طالب.الاسم="محمد"
	اظهر_(_طالب.العمر)
	اظهر_(_طالب.الاسم)
	تجربة().العمر=19
}