دالة البداية(){
	متغير الأسماء=إنشاء نص [4]
	متغير الأعمار=إنشاء صحيح [4]
	الأسماء.تعيين(0، "شريف")
	الأعمار.تعيين(0، 20)
	الأسماء.تعيين(1، "محمد")
	الأعمار.تعيين(1، 12)
	الأسماء.تعيين(2، "حسن")
	الأعمار.تعيين(2 ،6)
	الأسماء.تعيين(3، "أحمد")
	الأعمار.تعيين(3، 19)
	
	متغير ن=0
	طالما(ن<الأسماء.السعة){
		اظهر_(الأسماء.جلب(ن)+" ،وعمره "+الأعمار.جلب(ن).كنص())
		ن++
	}
	
}